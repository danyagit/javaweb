<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>MyFileServlet</servlet-name>
    <servlet-class>cn.itcast.web.servlet.MyFileServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyFileUploadServlet</servlet-name>
    <servlet-class>cn.itcast.web.servlet.MyFileUploadServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>MyFileServlet</servlet-name>
    <url-pattern>/MyFileServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyFileUploadServlet</servlet-name>
    <url-pattern>/MyFileUploadServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
